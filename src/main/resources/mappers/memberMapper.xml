<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.springProject.dao.MemberDAO">

	<select id="getMemberIdCheck" resultType="com.spring.springProject.vo.MemberVO">
		select * from member where mid = #{mid};
	</select>

	<select id="getmemberNickCheck" resultType="com.spring.springProject.vo.MemberVO">
		select * from member where nickName = #{nickName};
	</select>
	
	
